CREATE TABLE orders 
 (
 	order_id VARCHAR(255) PRIMARY KEY ,
 	client_name VARCHAR(255) , 
	client_email VARCHAR(255) , 
	client_event VARCHAR(255), 
	client_num_tickets INTEGER, 
	client_event_day VARCHAR(255), 
	client_card_num VARCHAR(50), 
	client_card_exp VARCHAR(50), 
	client_card_code VARCHAR(50), 
	client_purchase_cost VARCHAR(255),
	client_date_purchase DATE 

 );
 
--  DROP TABLE orders;
 
 
 SELECT * FROM orders;